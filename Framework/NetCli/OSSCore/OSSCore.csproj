<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<PackAsTool>true</PackAsTool>
	<ToolCommandName>osscore</ToolCommandName>
	<Description>OSSCore 模块解决方案 .Net 客户端工具</Description>
	<Version>2.6.12</Version>
	<PackageReleaseNotes>增加项目接口默认注释文档</PackageReleaseNotes>
	<PackageIconUrl>http://img1.osscore.cn/static/oss_net.png</PackageIconUrl>
	<PackageIcon>logo.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\logo.png" Link="logo.png">
      <PackagePath>\</PackagePath>
      <Pack>True</Pack>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Fluid.Core" Version="2.10.0" />
  </ItemGroup>

</Project>
