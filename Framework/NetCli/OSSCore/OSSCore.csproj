	<Version>3.1.0</Version>
	<PackageReleaseNotes>增加项目接口默认注释文档</PackageReleaseNotes>
	<PackageIconUrl>http://img1.osscore.cn/static/oss_net.png</PackageIconUrl>
	<PackageIcon>logo.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\logo.png" Link="logo.png">
